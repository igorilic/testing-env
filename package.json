{
  "name": "jstest",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-serve",
    "build": "webpack --mode production",
    "start": "serve --no-clipboard --listen 8880 dist",
    "lint": "eslint src",
    "flow": "flow",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage",
    "test:coverage:serve": "jest --coverage && serve --no-clipboard --listen 9999 coverage/lcov-report",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "prettier": "prettier  \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql)\" ",
    "format": "yarn prettier --write",
    "validate": "yarn lint && yarn test && yarn prettier --list-different && yarn flow"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-emotion": "^10.0.6",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.90.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-emotion": "^10.0.6",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-testing-library": "^5.4.4",
    "regenerator-runtime": "^0.13.1",
    "serve": "^10.1.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-serve": "^2.0.3"
  },
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-emotion": "^10.0.0",
    "react-loadable": "^5.5.0",
    "react-point": "^3.0.1"
  }
